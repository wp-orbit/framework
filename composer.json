{
    "name": "wp-orbit/framework",
    "description": "A WordPress development framework.",
    "type": "wordpress-muplugin",
    "authors": [
        {
            "name": "Marty Eason",
            "email": "dev@zawntech.com",
            "role": "Developer",
            "homepage": "http://zawntech.com"
        }
    ],
    "require": {
        "wp-orbit/ajax": "@dev",
        "wp-orbit/email": "@dev",
        "wp-orbit/forms": "@dev",
        "wp-orbit/meta-boxes": "@dev",
        "wp-orbit/post-types": "@dev",
        "wp-orbit/taxonomies": "@dev",
        "wp-orbit/tests": "@dev",
        "wp-orbit/views": "@dev"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/ajax.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/email.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/forms.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/meta-boxes.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/post-types.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/taxonomies.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/tests.git"
        },
        {
            "type": "git",
            "url": "git@github.com:wp-orbit/views.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "WPOrbit\\": "code/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WPOrbit\\Ajax\\": "../wp-orbit-components/ajax/code/",
            "WPOrbit\\Email\\": "../wp-orbit-components/email/code/",
            "WPOrbit\\Forms\\": "../wp-orbit-components/forms/code/",
            "WPOrbit\\MetaBoxes\\": "../wp-orbit-components/meta-boxes/code/",
            "WPOrbit\\PostTypes\\": "../wp-orbit-components/post-types/code/",
            "WPOrbit\\Taxonomies\\": "../wp-orbit-components/taxonomies/code/",
            "WPOrbit\\Tests\\": "../wp-orbit-components/tests/code/",
            "WPOrbit\\Views\\": "../wp-orbit-components/views/code/"
        }
    }
}
